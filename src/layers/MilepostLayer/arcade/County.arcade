var agolId = "12712f465fc44fb58328c6e0255ca27e";
var p = Portal("https://wsdot.maps.arcgis.com");

var match = Top(
  Within(
    $feature,
    FeatureSetByPortalItem(
      p,
      agolId,
      11,
      ["JURISDICT_LABEL_NM"],
      false
    )
  ),
  1
)

for (var f in match) {
  return f.JURISDICT_LABEL_NM
}

