{
  "name": "arcgis-mp",
  "private": true,
  "version": "0.0.0",
  "description": "Milepost map written with ArcGIS Maps API for JavaScript",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "watch": "vite build -- --watch",
    "preview": "vite preview",
    "build-and-preview": "npm run build && npm run preview -- --open",
    "clean": "git clean -dfx --exclude \"node_modules/**\" --exclude \".mailmap\"",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "publish": "npm run build && node tools/publish.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "spell": "cspell ."
  },
  "devDependencies": {
    "@arcgis/core": "^4.30.0",
    "@eslint/js": "^9.4.0",
    "@esri/arcgis-rest-request": "^4.2.3",
    "@fontsource/overpass": "^5.0.18",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/arcgis-rest-api": "^10.4.8",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "browserslist-to-esbuild": "^2.1.1",
    "cspell": "^8.9.1",
    "eslint": "^9.5.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.4.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "jsdom": "^24.1.0",
    "lefthook": "^1.6.18",
    "msw": "^2.3.1",
    "optionator": "^0.9.4",
    "prettier": "^3.3.2",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.1",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-static-copy": "^1.0.5",
    "vitest": "^1.6.0",
    "wsdot-route-utils": "^3.0.0"
  },
  "overrides": {
    "eslint": "$eslint",
    "glob": "^9.0.0"
  },
  "repository": {
    "url": "https://github.com/WSDOT-GIS/wsdot-mp-map.git",
    "type": "git"
  },
  "keywords": [
    "arcgis",
    "wsdot",
    "milepost",
    "map",
    "gis",
    "lrs",
    "linear-referencing"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "dependencies": {
    "@esri/calcite-components": "^2.9.0",
    "@fontsource/inconsolata": "^5.0.18",
    "@fontsource/lato": "^5.0.21",
    "@types/geojson": "^7946.0.14",
    "@wsdot/land-use-codes": "github:WSDOT-GIS/land-use",
    "@wsdot/web-styles": "github:WSDOT-GIS/wsdot-web-styles",
    "dms-conversion": "^3.1.3"
  }
}
