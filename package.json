{
  "name": "wsdot-mp-map",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --cors --open",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "preview": "npm run build && vite preview",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "gh-pages-publish": "npm run build && node tools/publish.mjs",
    "test": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage",
    "coverage-watch": "vitest --coverage"
  },
  "devDependencies": {
    "@fontsource/overpass": "^4.5.11",
    "@types/arcgis-rest-api": "^10.4.5",
    "@types/gh-pages": "^3.2.1",
    "@types/leaflet": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "gh-pages": "^5.0.0",
    "leaflet": "^1.9.3",
    "prettier": "^2.8.4",
    "simple-icons": "^8.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5",
    "wsdot-elc": "^4.0.0-0",
    "wsdot-route-utils": "^3.0.0"
  },
  "description": "Simple map for locating milepost data on WSDOT routes",
