{
  "name": "arcgis-mp",
  "private": true,
  "version": "0.0.0",
  "description": "Milepost map written with ArcGIS Maps API for JavaScript",
  "type": "module",
  "scripts": {
    "prepare": "tsx tools/copy-wsdot-webstyles.ts",
    "build": "vite build",
    "watch": "vite build -- --watch",
    "preview": "vite preview",
    "build-and-preview": "npm run build && npm run preview -- --open",
    "clean": "git clean -dfx --exclude \"node_modules/**\" --exclude \".mailmap\" --exclude \".env.local\"",
    "dev": "vite",
    "dev-browserstack": "vite --host",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "publish": "npm run build && node tools/publish.mjs",
    "test": "vitest run --bail 1",
    "test:watch": "vitest",
    "spell": "cspell ."
  },
  "devDependencies": {
    "@arcgis/core": "^4.30.9",
    "@eslint/js": "^9.10.0",
    "@esri/arcgis-rest-request": "^4.2.3",
    "@fontsource/overpass": "^5.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/arcgis-rest-api": "^10.4.8",
    "@types/browser-update": "^3.3.3",
    "@types/node": "^22.5.4",
    "@vitest/coverage-istanbul": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "browserslist-to-esbuild": "^2.1.1",
    "cspell": "^8.14.2",
    "eslint": "^9.10.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-sonarjs": "^2.0.2",
    "jsdom": "^25.0.0",
    "lefthook": "^1.7.15",
    "msw": "^2.4.5",
    "optionator": "^0.9.4",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.4",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^2.0.5",
    "wsdot-route-utils": "^3.0.0"
  },
  "repository": {
    "url": "https://github.com/WSDOT-GIS/wsdot-mp-map.git",
    "type": "git"
  },
  "keywords": [
    "arcgis",
    "wsdot",
    "milepost",
    "map",
    "gis",
    "lrs",
    "linear-referencing"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "dependencies": {
    "@esri/calcite-components": "^2.12.2",
    "@fontsource/inconsolata": "^5.1.0",
    "@fontsource/lato": "^5.1.0",
    "@types/geojson": "^7946.0.14",
    "@wsdot/land-use-codes": "github:WSDOT-GIS/land-use",
    "@wsdot/web-styles": "github:WSDOT-GIS/wsdot-web-styles",
    "browser-update": "^3.3.53",
    "calcite-point-symbols": "github:arcgis/calcite-point-symbols",
    "dms-conversion": "^3.1.3"
  }
}
