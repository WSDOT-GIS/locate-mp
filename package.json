{
  "name": "arcgis-mp",
  "private": true,
  "version": "0.0.0",
  "description": "Milepost map written with ArcGIS Maps API for JavaScript",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-and-preview": "npm run build && npm run preview -- --open",
    "clean": "git clean -dfx --exclude \"node_modules/**\" --exclude \".mailmap\"",
    "dev": "vite",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "prettier": "prettier .",
    "publish": "npm run build && node tools/publish.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "spell": "cspell .",
    "watch": "vite build -- --watch"
  },
  "devDependencies": {
    "@arcgis/core": "^4.29.7",
    "@eslint/js": "^8.57.0",
    "@esri/arcgis-rest-request": "^4.2.0",
    "@fontsource/overpass": "^5.0.17",
    "@tsconfig/node-lts": "^20.1.1",
    "@tsconfig/strictest": "^2.0.3",
    "@types/arcgis-rest-api": "^10.4.8",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "browserslist-to-esbuild": "^2.1.1",
    "cspell": "^8.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "wsdot-route-utils": "^3.0.0"
  },
  "repository": {
    "url": "https://github.com/WSDOT-GIS/wsdot-mp-map.git",
    "type": "git"
  },
  "keywords": [
    "arcgis",
    "wsdot",
    "milepost",
    "map",
    "gis",
    "lrs",
    "linear-referencing"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "dependencies": {
    "@esri/calcite-components": "^2.6.0",
    "@types/geojson": "^7946.0.14",
    "dms-conversion": "^3.1.3"
  }
}
