{
  "name": "arcgis-mp",
  "private": true,
  "version": "0.0.0",
  "description": "Milepost map written with ArcGIS Maps API for JavaScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-tsc": "tsc",
    "build-vite": "vite",
    "build": "tsc && vite build",
    "watch": "vite build -- --watch",
    "preview": "vite preview",
    "build-and-preview": "npm run build && npm run preview -- --open",
    "prettier": "prettier .",
    "prepublish": "npm run build",
    "publish": "node tools/publish.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --fix",
    "spell": "cspell ."
  },
  "devDependencies": {
    "@arcgis/core": "^4.28.10",
    "@eslint/js": "^8.56.0",
    "@esri/arcgis-rest-request": "^4.2.0",
    "@fontsource/overpass": "^5.0.16",
    "@tsconfig/node-lts": "^20.1.1",
    "@tsconfig/strictest": "^2.0.2",
    "@types/arcgis-rest-api": "^10.4.8",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-istanbul": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "browserslist-to-esbuild": "^2.1.1",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1",
    "wsdot-elc": "^4.0.0-0",
    "wsdot-route-utils": "^3.0.0"
  },
  "repository": {
    "url": "https://github.com/WSDOT-GIS/wsdot-mp-map.git",
    "type": "git"
  },
  "keywords": [
    "arcgis",
    "wsdot",
    "milepost",
    "map",
    "gis",
    "lrs",
    "linear-referencing"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense"
}
