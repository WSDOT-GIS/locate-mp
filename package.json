{
  "name": "arcgis-mp",
  "private": true,
  "version": "0.0.0",
  "description": "Milepost map written with ArcGIS Maps API for JavaScript",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "watch": "vite build -- --watch",
    "preview": "vite preview",
    "build-and-preview": "npm run build && npm run preview -- --open",
    "clean": "git clean -dfx --exclude \"node_modules/**\" --exclude \".mailmap\"",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prettier": "prettier .",
    "publish": "npm run build && node tools/publish.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "spell": "cspell ."
  },
  "devDependencies": {
    "@arcgis/core": "^4.29.10",
    "@eslint/js": "^9.2.0",
    "@esri/arcgis-rest-request": "^4.2.1",
    "@fontsource/overpass": "^5.0.18",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/arcgis-rest-api": "^10.4.8",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "browserslist-to-esbuild": "^2.1.1",
    "cspell": "^8.8.1",
    "eslint": "^9.2.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-sonarjs": "^1.0.3",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "lefthook": "^1.6.10",
    "optionator": "^0.9.4",
    "prettier": "^3.2.5",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.6.0",
    "wsdot-route-utils": "^3.0.0"
  },
  "overrides": {
    "eslint": "$eslint"
  },
  "repository": {
    "url": "https://github.com/WSDOT-GIS/wsdot-mp-map.git",
    "type": "git"
  },
  "keywords": [
    "arcgis",
    "wsdot",
    "milepost",
    "map",
    "gis",
    "lrs",
    "linear-referencing"
  ],
  "author": "Jeff Jacobson",
  "license": "Unlicense",
  "dependencies": {
    "@esri/calcite-components": "^2.8.0",
    "@types/geojson": "^7946.0.14",
    "dms-conversion": "^3.1.3"
  }
}
